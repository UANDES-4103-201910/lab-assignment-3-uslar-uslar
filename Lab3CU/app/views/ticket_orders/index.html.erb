<p id="notice"><%= notice %></p>

<h1>Listing Ticket Orders</h1>

<table>
  <thead>
    <tr>
      <th>Amount payed</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ticket_orders.each do |ticket_order| %>
      <tr>
        <td><%= ticket_order.amount_payed %></td>
        <td><%= ticket_order.user %></td>
        <td><%= link_to 'Show', ticket_order %></td>
        <td><%= link_to 'Edit', edit_ticket_order_path(ticket_order) %></td>
        <td><%= link_to 'Destroy', ticket_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket order', new_ticket_order_path %>
